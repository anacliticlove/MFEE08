<body>
  <!-- header start -->
  <header class="navbar">
    <!-- logo -->
    <div class="logo">
      <img src="/images/logo-w.png" alt="" />
      <span>- when do you trip?</span>
    </div>
    <!-- 搜尋欄 -->
    <div class="search">
      <input type="search" value="輸入城市、景點、體驗行程或活動名稱..." />
      <button>搜尋</button>
    </div>
    <!-- 登入/註冊 -->
    <div class="login">
      <a href="#">
        <span>登入/註冊</span>
      </a>
    </div>
    <!-- 快捷導航 -->
    <ul class="shortcut">
      <li>
        <a href="#">選擇你的專屬行程</a>
      </li>
      <li>
        <a href="#">尋找你的專屬導遊</a>
      </li>
      <li>
        <a href="#">購物車</a>
      </li>
    </ul>
  </header>
  <!-- header end -->
  <!-- banner start -->
  <div class="banner">
    <img src="/images/header-travel.jpg" alt="" />
